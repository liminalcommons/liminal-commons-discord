# Discord Application Credentials
# These can be obtained from the Discord Developer Portal
VITE_DISCORD_CLIENT_ID=your_discord_client_id
DISCORD_CLIENT_SECRET=your_discord_client_secret

# Webhook secrets for Discord integration
DISCORD_WEBHOOK_SECRET=your_webhook_secret
DISCORD_WEBHOOK_URL=your_discord_webhook_url
VITE_DISCORD_WEBHOOK_URL=your_discord_webhook_url

# When using cloudflared, update this to your tunnel URL (e.g., https://your-tunnel.trycloudflare.com)
CLOUDFLARE_TUNNEL_URL=your_cloudflare_tunnel_url

# Set to 'development' or 'production'
NODE_ENV=development